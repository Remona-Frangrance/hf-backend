{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "include": ["src", "server.ts"],
    "scripts": {
  "start": "tsc && node dist/index.js",
  "dev": "ts-node-dev --respawn src/index.ts"
}

  }
  